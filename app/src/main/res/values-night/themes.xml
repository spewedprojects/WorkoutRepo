<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.WorkoutRepo" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/black</item>
        <item name="colorSurface">@color/dark_primaryVariant</item>
        <item name="colorOnSurface">@color/white</item>
        <item name="colorPrimaryVariant">@color/dark_primary</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorAccent">@color/dark_onPrimary2</item>
        <item name="colorButtonNormal">@color/dark_switchbutton</item>
        <item name="colorBackgroundFloating">@color/dark_deletebutton</item>
        <item name="android:windowBackground">@color/darkwindow_background</item>
        <item name="colorSurfaceDim">@color/dialog_bg_dark</item>
        <item name="android:textColorHint">@color/dark_textHintcolor</item>
        <item name="colorOnPrimarySurface">@color/barChart_dark</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/dark_secondary</item>
        <item name="colorSecondaryVariant">@color/dark_secondaryVariant</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <item name="android:windowTranslucentStatus">false</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowTranslucentNavigation">false</item>
        <!-- Customize your theme here. -->
        <item name="colorPrimaryInverse">@color/white</item>
    </style>
</resources>